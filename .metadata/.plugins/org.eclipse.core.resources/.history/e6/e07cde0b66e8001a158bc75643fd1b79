package Q04;

public class Q04 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int M = 4;
		int N = 3;
		int k = 10;
		
		int [][] A = new int[N] [M];
		
		for (int i = 0  ; i < A.length; i++) {
			for (int j = 0 ; j < A[0].length ; j++) {
				A[i][j]= k+i;
			}
		}
		
		System.out.println("display matrix A");
		
	}

}
