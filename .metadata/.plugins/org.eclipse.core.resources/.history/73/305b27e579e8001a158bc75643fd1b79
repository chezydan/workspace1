package q10;

public class q10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [][] A = { {1, 1, 1, 1, 2,1},
		               {1, 4, 1,  2, 1,2  },
		              { 10, 1, 16 , 1,2, 1 }  };	
		
      int [][] B = { {1,10,1},
        {10,100,1},
          { 10,1,16 }  };
		
		
		
	}
public static int isPerfect(int x, int y , int[][] mat)	{
	int maxRow = mat.length-1;
	int maxCol = mat[0].length-1;
	
	if ( (x+2)> maxCol || (y +2) > maxRow)
		return 0;
	
	return 1;
}

}
