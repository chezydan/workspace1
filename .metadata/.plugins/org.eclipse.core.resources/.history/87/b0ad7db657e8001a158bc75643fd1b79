package q03;
import java.util.Random;

public class Q03 {
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int MAX=1000;
		int MINIMUM=100;
		int [] Ar = new int [10];
		for (int i = 0; i < 10; i++) {
			Ar[i] =MINIMUM + (int)(Math.random() * ((MAX - MINIMUM) + 1));
			System.out.println(Ar[i]);
		}
			
		}
		
	}


